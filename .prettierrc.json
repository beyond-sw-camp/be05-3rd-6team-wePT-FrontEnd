{
  "$schema": "https://json.schemastore.org/prettierrc",

  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParents": "avoid",
  "vueIndentScriptAndStyle": false,
  "endOfLine": "auto"
}